local NotificationSound = Instance.new("Sound",game:GetService("SoundService"))
NotificationSound.SoundId = "rbxassetid://3398620867"
NotificationSound.Volume = 1

Notifications = {}

local notificationGui = Instance.new("ScreenGui",game.CoreGui)
function Notify(Header,Body,Delay)
    NotificationSound:Play()
    local Frame = Instance.new("Frame",notificationGui)
    local Frame2 = Instance.new("Frame",Frame)
    local Image = Instance.new("ImageLabel",Frame2)
    local Corner1 = Instance.new("UICorner",Frame)
    local Corner2 = Instance.new("UICorner",Frame2)
    local HText = Instance.new("TextLabel",Frame2)
    local BText = Instance.new("TextLabel",Frame2)
    Frame.Size = UDim2.new(0.199, 0,0.11, 0)
    Frame.Position = UDim2.new(1.78, 0,0.001, 0)
    Frame.BackgroundColor3 = Color3.fromRGB(61, 61, 61)
    Frame.BackgroundTransparency = 1
    Frame2.AnchorPoint = Vector2.new(0.5,0.5)
    Frame2.Size = UDim2.new(0.975,0,0.934,0)
    Frame2.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    Frame2.Position = UDim2.new(0.5, 0, 0.5, 0)
    HText.Size = UDim2.new(0.705, 0,0.309, 0)
    HText.Position = UDim2.new(0.267, 0, 0.081, 0)
    HText.BackgroundTransparency = 1
    HText.TextColor3 = Color3.fromRGB(255,255,255)
    HText.Font = Enum.Font.GothamBlack
    HText.TextSize = HText.AbsoluteSize.X * 0.12
    HText.TextXAlignment = "Left"
    BText.Size = UDim2.new(0.705, 0,0.562, 0)
    BText.TextSize = BText.AbsoluteSize.X * 0.065
    BText.TextWrapped = true
    BText.Position = UDim2.new(0.267, 0, 0.465, 0)
    BText.TextXAlignment = "Left"
    BText.TextYAlignment = "Top"
    BText.BackgroundTransparency = 1
    BText.TextColor3 = Color3.fromRGB(255,255,255)
    BText.Font = Enum.Font.Gotham
    Image.BackgroundTransparency = 1
    Image.Size = UDim2.new(0.167, 0, 0.582, 0)
    Image.Position = UDim2.new(0.046, 0, 0.202, 0)
    Image.Image = "http://www.roblox.com/asset/?id=10196601290"
    HText.Text = Header
    BText.Text = Body
    for i = 1,100 do
        task.wait()
        Frame.Position = Frame.Position - UDim2.new(0.01,0,0,0)
    end
    wait(Delay)
    for i = 1,100 do
        task.wait()
        Frame.Position = Frame.Position + UDim2.new(0.01,0,0,0)
    end
    Frame:Destroy()
end
 
Notify("Cat Prompt","Welcome to Cat Prompt",2)


