_G.detected = false

for i,v in pairs(games) do
    if v.gameid == game.GameId then
        _G.detected = true
        Notify("Cat Prompt","Game Found",2)
        loadstring(game:HttpGet(v.LString, true))()
    end
end

if _G.detected == false then
    Notify("Cat Prompt","Game Not Found",2)
end

 
